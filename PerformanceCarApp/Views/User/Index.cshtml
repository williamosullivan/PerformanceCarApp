@model IEnumerable<PerformanceCarApp.Models.User>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Name
        </th>
        <th>
            Make
        </th>
        <th>
            Model
        </th>
        <th>
        <th>
            Birthday
        </th>
        <th>
            Gender
        </th>
        <th>
            Horsepower
        </th>
        <th>
            Quarter Mile
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.TextArea((String)ViewBag.UserName)
        </td>
        <td>
            @Html.TextArea((String)ViewBag.UserCarMake)
        </td>
        <td>
            @Html.TextArea((String)ViewBag.UserCarModel)
        </td>
        <td>
            @Html.TextArea((String)ViewBag.BDay)
        </td>
        <td>
            @Html.TextArea((String)ViewBag.Gender)
        </td>
        <td>
            @Html.TextArea((String)ViewBag.UserHP)
        </td>
        <td>
            @Html.TextArea((String)ViewBag.UserQM)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.UserID }) |
            @Html.ActionLink("View Profile", "Details", new { id=item.UserID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.UserID })
        </td>
    </tr>
}

</table>
