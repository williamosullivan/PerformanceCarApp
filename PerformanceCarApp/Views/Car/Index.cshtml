@model IEnumerable<PerformanceCarApp.Models.Car>

@{
    ViewBag.Title = "Car Index";
}
<link rel="stylesheet" type="text/css" href="~/Content/style.css">


<h2>Car Index</h2>

<h2>Build Your Ride!</h2>


<div>
    <center>
        @Html.DropDownList("CarMakeList", (IEnumerable<SelectListItem>)ViewBag.SelectListCarMakes)
        @Html.DropDownList("CarModelList", (IEnumerable<SelectListItem>)ViewBag.SelectListCarModels)
        <button type="submit" value="submit" name="Submit" >Submit</button>
    </center>
    <br /><br />
</div>
